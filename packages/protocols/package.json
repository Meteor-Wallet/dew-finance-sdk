{
  "name": "@dew-finance/protocols",
  "version": "0.1.5",
  "description": "Protocol adapters and helpers for Dew Finance SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./burrow": {
      "types": "./dist/burrow/index.d.ts",
      "import": "./dist/burrow/index.js"
    },
    "./ref-finance": {
      "types": "./dist/ref-finance/index.d.ts",
      "import": "./dist/ref-finance/index.js"
    }
  },
  "repository": {
    "url": "https://github.com/Meteor-Wallet/dew-finance-sdk.git",
    "type": "git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@dew-finance/core": "0.1.5",
    "big.js": "^7.0.1",
    "@near-js/providers": "^2.0.1",
    "@near-js/transactions": "^2.0.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
